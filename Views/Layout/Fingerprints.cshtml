
<div class="menu-main Fingerprints dispay_none" style="background-color:white;margin-top: 10px;">
    <div class="container_12">

        <!-- Header -->
        <div id="header" class="grid_12 alpha">
            <h1>
                <img src="~/img/SiYoSa-Logo.png" alt="Siyosa logo" width="30" /> Finferprint database & Improvement tool
                @*<img id="global_spinner" alt="loading" src="~/img/input-spinner.gif" width="50" />*@
            </h1>
            <!--<div class="clear"></div>-->
            <div id="login_box">
                <div class="row">
                    <div class="col-md-2">
                        <input id="username" type="text" class="form-control" placeholder="username" value="" />
                    </div>
                    <div class="col-md-2">
                        <input id="password" type="password" class="form-control" placeholder="password" value="" />
                    </div>
                    <div class="col-md-2">
                        <input id="site_id" type="text" class="form-control" placeholder="site ID" value="" />
                    </div>
                    <div class="col-md-2">
                        <button id="fetch_fps" class="btn btn-danger">Show</button>
                    </div>
                    <div class="col-md-2">
                        <button id="showfingprint" class="btn btn-danger">Show Delete Fin</button>
                    </div>
                    <div class="col-md-2">
                        <button id="downloadfingerprint" class="btn btn-danger">Download Fin</button>
                    </div>
                </div>
                <div class="row displayfingerprint" style="margin-top:10px; height:350px; overflow:auto;display:none">
                    <h4 style="text-align:center; color:red; font-weight:bold">Delete Fingerprint</h4>
                    <p style="text-align:center"><button id="deleteallfingprint" class="btn btn-danger">Delete All</button></p>
                    <table class="table" style="width: 98%; margin:0 auto;">
                        <thead>

                            <tr>
                                <th>ID</th>
                                <th>MAC</th>
                                <th>Lat</th>
                                <th>Lng</th>
                                <th>Delete</th>
                            </tr>
                        </thead>
                        <tbody class="showfingerprint">
                            <tr style="text-align:center; height:50px; font-size:20px"><th colspan="5"> Server not Data, below is data test </th></tr>
                            <tr class="clearprint">
